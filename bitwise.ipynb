    "class Solution:\n",
    "    def minFlips(self, a: int, b: int, c: int) -> int:\n",
    "        print(f\"A:{bin(a)}\")\n",
    "        print(f\"B:{bin(b)}\")\n",
    "        print(f\"C:{bin(c)}\")\n",
    "        if a | b == c:\n",
    "            return 0\n",
    "        else:\n",
    "            return 1"
   ]
